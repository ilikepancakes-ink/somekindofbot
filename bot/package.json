{
  "name": "multi-purpose-discord-bot",
  "version": "1.0.0",
  "description": "A multi-purpose Discord bot with moderation, fun, and info commands",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "watch": "tsc --watch",
    "deploy": "npm run build && concurrently \"npm start\" \"npm run serve-management\" \"cd ../website && node server.js\"",
    "serve-website": "cd ../website && node server.js",
    "serve-management": "node dist/server.js"
  },
  "keywords": [
    "discord",
    "bot",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.2",
    "discord.js": "^14.14.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "sqlite3": "^5.1.6",
    "systeminformation": "^5.30.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^20.10.0",
    "concurrently": "^8.2.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.0"
  }
}
